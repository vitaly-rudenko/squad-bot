{
  "name": "squad-bot",
  "version": "5.2.0",
  "description": "Telegram bot assistant for squads",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "npm run umzug db:migrate && node app.js",
    "dev": "npm run umzug db:migrate && nodemon --legacy-watch --ignore test/ app.js",
    "dev:https": "ENABLE_TEST_HTTPS=true npm run dev",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --recursive test/unit",
    "test:unit:dev": "nodemon --delay 0.5 --exec 'npm run test:unit'",
    "test:integration": "mocha --recursive test/integration",
    "test:integration:dev": "nodemon --delay 1 --exec 'npm run test:integration'",
    "umzug": "node ./umzug/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaly-rudenko/squad-bot.git"
  },
  "author": "Vitaly Rudenko",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/vitaly-rudenko/squad-bot/issues"
  },
  "homepage": "https://github.com/vitaly-rudenko/squad-bot#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "chance": "^1.1.8",
    "common-tags": "^1.8.2",
    "deep-equal-in-any-order": "^1.1.15",
    "mocha": "^9.1.4",
    "node-fetch": "^3.2.0",
    "nodemon": "^2.0.15",
    "sinon": "^13.0.1",
    "unique-names-generator": "^4.7.1"
  },
  "dependencies": {
    "@types/multer": "^1.4.11",
    "@vitalyrudenko/templater": "^1.0.0-pre.4",
    "cors": "^2.8.5",
    "dotenv": "^12.0.3",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "markdown-escapes": "^2.0.1",
    "multer": "^1.4.4",
    "nanoid": "^5.0.5",
    "nanoid-dictionary": "^5.0.0-beta.1",
    "pg": "^8.7.1",
    "pino": "^7.9.1",
    "pino-pretty": "^7.5.4",
    "superstruct": "^1.0.3",
    "telegraf": "^4.15.3",
    "umzug": "^3.0.0"
  }
}
